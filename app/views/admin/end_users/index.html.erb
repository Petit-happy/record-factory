<div class="container">
  <div class="admin-end-users">
    <h2><span class="label label-info">会員様一覧</span></h2>
    <table class="" border="1" width="100%" height="50" align="center">
      <thead>
        <tr>
          <th>ID</th>
          <th>氏名</th>
          <th>メールアドレス</th>
          <th>住所</th>
          <th>郵便番号</th>
          <th>電話番号</th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <% @end_users.each do |end_user| %>
        <tr align="center">
          <td><%= end_user.id %></td>
          <td><%= end_user.family_name_kanji %><%= end_user.given_name_kanji %></td>
          <td><%= end_user.email %></td>
          <td><%= end_user.address %></td>
          <td><%= end_user.post_code %></td>
          <td><%= end_user.phone_number %></td>
            <td><%= link_to "詳細", admin_end_user_path(end_user), class:"btn-sm btn-primary" %>　　<%= link_to "編集", edit_admin_end_user_path(end_user.id), class:"btn-sm btn-info" %></td>
        </tr>
        <% end %>
            <%= paginate @end_users %>
      </tbody>
    </table>
  </div>
</div>
