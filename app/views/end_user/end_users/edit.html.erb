<h1>ユーザ情報編集</h1>
<%= form_for([:end_user, @end_user]) do |f| %>
    <div>名前（姓） <%= f.text_field :family_name_kanji %></div>
    <div>名前（名） <%= f.text_field :given_name_kanji %></div>
    <div>名前（姓カナ） <%= f.text_field :family_name_kana %></div>
    <div>名前（姓カナ） <%= f.text_field :given_name_kana %></div>
    <div>郵便番号 <%= f.text_field :post_code %></div>
    <div>電話番号<%= f.text_field :phone_number %></div>
    <div>メールアドレス <%= f.email_field :email %></div>

    <%= f.submit "変更を保存", class:"btn btn-lg btn-block btn-base-1 mt-20" %>
<% end %>